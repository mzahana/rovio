<?xml version="1.0" encoding="UTF-8"?> 
<launch>
  <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
  <param name="filter_config" value="$(find rovio)/cfg/rovio_rpi.info"/>
  <param name="camera0_config" value="$(find rovio)/cfg/arducam_cam0.yaml"/>
  <!--param name="camera1_config" value="$(find rovio)/cfg/euroc_cam1.yaml"/-->
  <remap from="imu0" to="/imu/data_raw" />
  <remap from="cam0/image_raw" to="/left_image" />
  <remap from="cam1/image_raw" to="/right_image" />
  </node>
</launch>
